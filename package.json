    "@types/bull": "^3.15.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/stripe": "^8.0.416",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
